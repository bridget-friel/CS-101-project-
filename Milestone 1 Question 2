def dna2rna(dna):
    rna = ''
    dna = dna.upper()
    for symbol in dna:
        if symbol == 'T':
            rna = rna + 'U'
        elif symbol == 'G':
            rna = rna +'G'
        elif symbol == 'A':
            rna = rna + 'A'
        elif symbol == 'C':
            rna = rna + 'C'
    return rna 
        
