prob = 0
def count_dom_phenotype(genotypes):
    for index in range(len(genotypes)):
        if index == 0:
            prob = genotypes[0]*2
        if index == 1:
            prob = prob + genotypes[1]*2
        if index == 2:
            prob = prob + genotypes[2]*2
        if index == 3:
            prob= prob + (genotypes[3]*2*.75)
        if index == 4:
            prob= prob + genotypes[4]*2*.5
        if index == 5:
            prob = prob + 0 
    return prob
